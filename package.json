{
  "name": "hfex-commitlint-config",
  "version": "1.0.1",
  "description": "git commitlint",
  "main": "./src/index.js",
  "scripts": {
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:UzumakiHan/hfex-commitlint-config.git"
  },
  "keywords": [],
  "files": [
    "src",
    "package.json"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/types": "^17.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/load": "^17.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "core-js": "^3.30.2",
    "eslint": "^8.42.0",
    "eslint-config-hfex": "^1.0.2",
    "hfex-prettier-config": "^1.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "hfex"
    ]
  },
  "prettier": "hfex-prettier-config",
  "lint-staged": {
    "src/**/*.{ts,js,json,vue,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,vue,scss,css,less,json,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
